{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:profitminer/pm-base.git"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "aws/aws-sdk-php": "^3.71",
        "aws/aws-sdk-php-laravel": "^3.3",
        "barryvdh/laravel-debugbar": "^3.2",
        "bogardo/mailgun": "^5.0",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "lab404/laravel-impersonate": "^1.2",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "league/csv": "^9.1",
        "league/flysystem-aws-s3-v3": "^1.0",
        "mailgun/mailgun-php": "^2.6",
        "php-http/guzzle6-adapter": "^1.1",
        "pion/laravel-chunk-upload": "^1.2",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^3.3",
        "sofa/eloquence-base": "^5.6",
        "spatie/laravel-activitylog": "^3.1",
        "spatie/laravel-medialibrary": "^7.5",
        "twig/twig": "^2.0",
        "twilio/sdk": "^5.24",
        "profitminer/base": "^1.0",
        "fzaninotto/faker": "^1.4"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^5.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "Laravel\\Dusk\\DuskServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
